{
    "mode": "All",
    "policyRule": {
      "if": {
        "allOf": [
          {
            "field": "type",
            "like": "Microsoft.Network/*"
          },
          {
            "not": {
              "field": "type",
              "equals": "Microsoft.Network/networkInterfaces"
            }
          }
        ]
      },
      "then": {
        "effect": "deny"
      }
    },
    "parameters": {}
}